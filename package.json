{
  "name": "fnutil",
  "version": "0.3.0",
  "description": "ES6 Functional Utilities",
  "main": "./",
  "repository": "https://github.com/f5io/fnutil",
  "engines": {
    "node": ">=4.0.0"
  },
  "files": [
    "index.js",
    "core",
    "csp",
    "decorators",
    "observable",
    "utils"
  ],
  "scripts": {
    "compile": "babel --out-dir ./ src",
    "coverage": "isparta cover --report lcov ./test -- test/**/*.spec.js",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test": "npm run compile && node ./test test/**/*.spec.js | tap-bail | tap-difflet",
    "watch": "watch 'npm test' src test",
    "prepublish": "npm test"
  },
  "author": "Joe Harlow",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.34",
    "coveralls": "^2.11.4",
    "glob": "^6.0.1",
    "isparta": "^3.5.1",
    "tap-bail": "0.0.0",
    "tap-difflet": "^0.3.0",
    "tape": "^4.2.0",
    "watch": "^0.16.0"
  }
}
