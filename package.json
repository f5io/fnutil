{
  "name": "fnutil",
  "version": "0.1.5",
  "description": "ES6 Functional Utilities",
  "main": "lib/",
  "repository": "https://github.com/f5io/fnutil",
  "engines" : { "node" : ">=4.0.0" },
  "scripts": {
    "compile": "babel --stage 0 --out-dir lib src",
    "test": "npm run compile && babel-tape-runner test/**/*.js | tap-difflet",
    "watch": "watch 'npm test' src test",
    "prepublish": "npm run compile",
    "postinstall": "npm run compile"
  },
  "author": "Joe Harlow",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.5.6",
    "babel-runtime": "^5.5.6",
    "babel-tape-runner": "^1.2.0",
    "tap-difflet": "^0.3.0",
    "tape": "^4.2.0",
    "watch": "^0.16.0"
  }
}
